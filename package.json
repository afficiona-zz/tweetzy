{
  "name": "afficiona-tweety",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "classnames": "2.2.5",
    "copyfiles": "^1.2.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "get-size": "2.0.2",
    "http": "0.0.0",
    "https": "^1.0.0",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "moment": "^2.19.4",
    "npm-run-all": "4.1.2",
    "query-string": "5.0.1",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-websocket": "^2.0.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "replace-in-file": "^3.0.0",
    "twit": "^2.2.9",
    "ws": "3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "conventional-github-releaser": "2.0.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.8.0",
    "eslint-config-react-app": "2.0.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "node-sass-chokidar": "0.0.3",
    "prettier": "1.7.4",
    "react-scripts": "1.0.17",
    "standard-version": "4.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-client": "npm-run-all -p watch-css start-js",
    "start": "node server.js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js copyfiles && node replace.js",
    "copyfiles": "copyfiles -f build/index.html ./",
    "test": "npm run lint",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "deploy": "sh bin/deploy.sh",
    "prerelease": "sh bin/prerelease.sh",
    "release": "standard-version",
    "postrelease": "sh bin/postrelease.sh"
  }
}
